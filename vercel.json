
{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/test",
      "dest": "/public/test.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
 
   
}
